{
  "manifest_version": 2,
  "name": "Save SVG",
  "description": "Save inline SVG images",
  "version": "0.1.3",
  "homepage_url": "https://github.com/martinthomson/save-svg",
  "icons": { "48": "icon.svg", "96": "icon.svg" },

  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },

  "permissions": [
    "activeTab",
    "contextMenus",
    "menus"
  ]
}
